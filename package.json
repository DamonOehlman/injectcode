{
  "name": "injectcode",
  "version": "1.2.2",
  "description": "A simple command-line tool for injecting code snippets into markdown files",
  "main": "index.js",
  "scripts": {
    "test": "tape test/matcher.js",
    "gendocs": "gendocs > README.md",
    "demo": "cd examples && ../bin/injectcode < sample.md && cd ..",
    "demo2": "cd examples && ../bin/injectcode < sample-links.md && cd ..",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "bin": {
    "injectcode": "bin/injectcode"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DamonOehlman/injectcode.git"
  },
  "keywords": [
    "cli",
    "inject",
    "markdown"
  ],
  "author": "Damon Oehlman <damon.oehlman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DamonOehlman/injectcode/issues"
  },
  "dependencies": {
    "async": "^2.1.4",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "tape": "^4.6.2"
  },
  "snyk": true
}